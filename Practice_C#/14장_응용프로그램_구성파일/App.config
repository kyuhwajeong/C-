<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="myAppSettings"
             type="CSharpPhrase.CustomSection.MyAppSettings,14장_응용프로그램_구성파일"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="EnableTrace" value="true" />
    <add key="Timeout" value="30000" />
  </appSettings>
  <myAppSettings>
    <traceOption enabled="true"
                 filePath="C:\MyApp\Trace.log"
                 bufferSize="10240" />
  </myAppSettings>
</configuration>